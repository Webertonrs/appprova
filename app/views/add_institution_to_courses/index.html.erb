<p id="notice"><%= notice %></p>

<h1>Add Institution To Courses</h1>

<table>
  <thead>
    <tr>
      <th>Institution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_institution_to_courses.each do |add_institution_to_course| %>
      <tr>
        <td><%= add_institution_to_course.institution %></td>
        <td><%= link_to 'Show', add_institution_to_course %></td>
        <td><%= link_to 'Edit', edit_add_institution_to_course_path(add_institution_to_course) %></td>
        <td><%= link_to 'Destroy', add_institution_to_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

